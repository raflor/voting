extends layout

block content
  .flex-container
    .flex-item
      div(style="width:70%;overflow:hidden;margin-left:15%;")
        button.btn.btn-default.btn-sm(type="button", style="float:right;margin-bottom:5px;")
          |Filter
      #list
        .list-group(role="tablist", id="accordion")
          for poll in polls
            include poll2
    .flex-item
      canvas#myChart(width="400", height="400")
block script
  script(type='text/javascript', src='controllers/votesController.client.js')